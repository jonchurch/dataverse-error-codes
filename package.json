{
  "name": "dataverse-error-codes",
  "version": "1.0.0",
  "description": "Error codes for Microsoft Dataverse Web Platform",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "scrape": "node ./scripts/scrape.js",
    "build:esm": "babel src src/index.js --out-file dist/esm/index.js",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src/index.js --out-file dist/cjs/index.js",
    "build": "npm run build:esm && npm run build:cjs"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "dataverse",
    "error codes",
    "power platform"
  ],
  "author": "jonchurch",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "cheerio": "^1.0.0-rc.12",
    "cross-env": "^7.0.3"
  }
}
